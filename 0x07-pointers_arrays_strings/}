#include "main.c"

/**
 * _memset - fills memory with a constant byte
 * @s: address oof the memory to fill
 * @b: Value to fill into the memory
 * @n: number of bytes to be filled
 * Return: a pointer to the memory area or s.
 */
char *_memset(char *s, char b, unsigned int n)
{
	int i;

	for (i = 0; i < n; i++)
	{
		(s + i): =             // increase address to input the value
	}

	return (0);
}
